{
  "name": "create-anything",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cd apps/web && npm run dev",
    "build": "cd apps/web && npm run build",
    "typecheck": "cd apps/web && npm run typecheck",
    "start": "cd apps/web && npm run start",
    "postinstall": "patch-package"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "overrides": {
    "p-map": "4.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "patch-package": "^8.0.1"
  }
}
